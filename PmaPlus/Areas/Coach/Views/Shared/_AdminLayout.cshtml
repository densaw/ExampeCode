</head>
<body ng-cloak id="bodyMain" ng-app="MainApp" ng-controller="MainController" class="skin-1  {{expanded | nav: '':'mini-navbar'}}">
    <!-- PageClass give you ability to specify custom style for specific view based on action -->
    <toaster-container toaster-options="{'position-class': 'toast-top-full-width'}"></toaster-container>
    <div id="wrapper" class="@Html.PageClass()">
        <!-- Navigation -->
        @Html.Partial("_Navigation")
        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg @ViewBag.SpecialClass @Html.IsSelectedSide(controller: "Home", action: "Dashboard")">
            <!-- Top Navbar -->
            @Html.Partial("_TopNavbar")
            <!-- Main view -->
            @RenderBody()
            <!-- Footer -->
            @Html.Partial("_Footer")
        </div>
        <!-- End page wrapper-->
    </div>
    <!-- End wrapper-->
    <!-- Section for main scripts render -->
    @*Scripts.Render("~/bundles/bootstrap")*@
    @Scripts.Render("~/plugins/slimScroll")
    @Scripts.Render("~/bundles/inspinia")
    @Scripts.Render("~/bundles/jquery-session")
    @Scripts.Render("~/bundles/SysAdminCtr")
    @Scripts.Render("~/bundles/ClubAdminCtr")
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/plugins/fileinput")



    <!-- Handler for local scripts -->
    @RenderSection("scripts", required: false)
    <style>
        .sidebar-content .wrapper,
        .wrapper.sidebar-content {
            padding-right: 300px !important;
        }

        .sidebard-panel {
            width: 250px;
            background: #ebebed;
            padding: 10px 10px;
            position: absolute;
            right: 0;
        }

        .title-modal {
            font-family: 'Open Sans';
            font-size: 12px;
            font-style: normal;
            font-variant: normal;
            font-weight: 400;
            line-height: 12px;
        }

        .container-canter {
            display: table;
            width: 100%;
            height: 100%; /* For at least Firefox */
            min-height: 100%;
        }

        .inner-container-center {
            display: table-cell;
            vertical-align: middle;
        }

        .skin-1 .navbar-top-links li:last-child a:hover {
            background-color: #f4f6fa !important;
            color: #333333;
        }
    </style>

    <script>
        $(document).ready(function () {





            toastr.options = {
                closeButton: true,
                debug: false,
                progressBar: false,
                positionClass: "toast-top-full-width",
                onclick: null,
                showDuration: 400,
                hideDuration: 1000,
                timeOut: 7000,
                extendedTimeOut: 1000,
                showEasing: "swing",
                hideEasing: "linear",
                showMethod: "fadeIn",
                hideMethod: "fadeOut"
            }
        });
    </script>
</body>

</html>

<script>
    var idleTime = 0;
    $(document).ready(function () {
        var idleInterval = setInterval(timerIncrement, 60000);
        $(this).mousemove(function (e) {
            idleTime = 0;
        });
        $(this).keypress(function (e) {
            idleTime = 0;
        });
    });

    function timerIncrement() {
        idleTime = idleTime + 1;
        if (idleTime > 9)
            window.location.replace('@Url.Action("LockScreen", "Account", new {area = ""})');

    }
</script>
