@{
    ViewBag.Title = "PMA+";
    Layout = "~/Areas/Coach/Views/Shared/_AdminLayout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Wizard Page</h2>
        <ol class="breadcrumb">
            <li>
                <a>Home</a>
            </li>
            <li class="active">
                <strong>Wizard Page</strong>
            </li>
        </ol>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a class="btn btn-primary" ng-href="@Url.Action("MatchReports", "Home", new { area = "Coach" })">Back to Match Reports</a>
            <a class="btn btn-primary" ng-href="@Url.Action("ReportsWizard", "Home", new { area = "Coach" })/{{currId}}">Add Match Report</a>
        </div>
    </div>

</div>
