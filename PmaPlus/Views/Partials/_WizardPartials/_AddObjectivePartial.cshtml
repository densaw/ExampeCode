
<div ng-controller="CurrObjectiveController" ng-cloak>

    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Add Objective </h5>
                    <div class="ibox-tools" style="margin-top: -5px;">
                        
                    </div>
                </div>
                <div class="table-responsive">
                    <div class="ibox-content">
                        <div class="row">
                            <div class="col-sm-9 m-b-xs">

                            </div>
                        </div>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Picture</th>
                                    <th>Player Name</th>
                                    <th>Objectives</th>
                                    <th>Outcome</th>
                                    <th>Feedback</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr dir-paginate="item in items | filter:q | itemsPerPage: itemsPerPage" total-items="totalItems">
                                    <td>
                                        <div style="width: 100px; height: 100px; position: relative; overflow: hidden; border-radius: 50%;" class="img-circle pull-left">
                                            <img class="prifile" ng-src="{{item.picture}}" style-parent />
                                        </div>
                                    </td>
                                    <td>{{item.name}}</td>
                                    <td><input type="text" ng-model="item.objective" /></td>
                                    <td>{{item.outcome}}</td>
                                    <td>{{item.feedBack}}</td>
                                </tr>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>

        </div>
    </div>
    
</div>
